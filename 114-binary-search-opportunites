public static Integer search(List<Opportunity> opportunities, Integer target){
    Integer left =0;
    Integer right = opportunities.size()-1;
    while(left<right){
        Integer index = (right-left)/2;
        Opportunity o= opportunities.get(index);
        if(o.amount<target){
            left = index;
        }
        else if(o.amount == target){
            return index;
        }
        else if(o.amount>target){
            right = index;
        }
        else{
            return -1;
        }
    }
    // for(Integer i =opportunities.size()-1; i>=0; i--){
    //     if(o.amount<target){
    //         return -1;
    //     }


    // }
    return -1;
    // solution here
}