public boolean isAnagram(String s1, String s2) {
    //Code here
    Integer[] anagram = new Integer[26];
    for(Integer i = 0; i<s1.size();i++){
        anagram[s1.charAt(i)-'a'] +=1;
    }
    for(Integer i = 0; i<s2.size();i++){
        anagram[s2.charAt(i)-'a'] -=1;
    }
    for(Integer i = 0; i<anagram.size();i++){
        if(anagram[i] != 0){
            return false;
        }
    }
    return true;
}