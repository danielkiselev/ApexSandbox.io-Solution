public static Boolean sameString(String str1, String str2){
    String r1 = process(str1);
    String r2 = process(str2);
    
    return r1.equals(r2);
    // solution
}

public String process(String input){
    String res = '';
    Integer inputL = input.length();
    for(Integer z = 0; z<inputL; z++){
        String curr = input.substring(z,z+1);
        if((curr == '#')){
            Integer resL = res.length();
            String temp = res.substring(0,resL);
            res = temp;
            continue;
        }
        else{
            res+=curr;
        }
    }
    return res;
}